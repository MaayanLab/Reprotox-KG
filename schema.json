					"type": "text"
				},
				{
					"label": "act type",
					"text": "${act_type}",
					"type": "text"
				},
				{
					"label": "act relation",
					"text": "${act_relation}",
					"type": "text"
                },
				{
					"label": "act source",
					"text": "${act_source}",
					"type": "text"
                }	
			]
		},
		{
