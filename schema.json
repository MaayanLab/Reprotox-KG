					"type": "text"
				},
				{
					"label": "Resource",
					"text": "${resource}",
					"href": "https://maayanlab.cloud/sigcom-lincs",
					"type": "link"
				},
				{
					"label": "Mean CD Coefficient",
					"text": "${mean_CD_coefficient}",
					"type": "text"
				}	
			]
		},
		{
			"match": ["LINCS Drugs Cosine Similarity"],
			"order": ["cosine_similarity", "DESC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Name",
					"text": "${label}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
					"type": "text"
				},
				{
					"label": "Target Label",
					"text": "${target_label}",
					"type": "text"
				},
				{
					"label": "Resource",
					"text": "${resource}",
