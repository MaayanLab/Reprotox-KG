					"type": "link"
				},
				{
					"label": "Cosine Similarity",
					"text": "${cosine_similarity}",
					"type": "text"
				}	
			]
		},
		{
			"match": ["FDA Adverse Event Reporting System (Female)"],
			"palette": {
				"main": "#e040fb"
			},
			"order": ["llr_ratio", "DESC"],
			"display": [
				{
					"label": "ID",
					"text": "${id}",
					"type": "text"
				},
				{
					"label": "Name",
					"text": "${label}",
					"type": "text"
				},
				{
					"label": "Source Label",
					"text": "${source_label}",
