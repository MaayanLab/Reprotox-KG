	"header": {
		"title": "<h5 style='color:white;font-size:1.5rem;margin-left:5px'><b>Toxicology Screening Knowledge Graph for Structural Birth Defects</b></h5>",
		"icon": {
			"favicon": "https://github.com/MaayanLab/Reprotox-KG/raw/main/Reprotox.png",
			"src": "https://github.com/MaayanLab/Reprotox-KG/raw/main/Reprotox.png",
			"alt": "CFDE logo",
			"width": 70,
			"height": 70
		},
		"tabs": [
			{
				"endpoint": "/",
				"label": "Knowledge Graph",
				"type": "page",
				"component": "KnowledgeGraph",
				"props": {
					"init_function": "initialize_kg"
				}
			},
			{
				"endpoint": "/downloads",
				"label": "Download Assets",
				"type": "markdown",
				"component": "Markdown",
				"props":{
					"src": "https://raw.githubusercontent.com/MaayanLab/Reprotox-KG/main/markdown/download.md"
				}
			}
		],
		"subheader": [
			{
				"label": "LINCS",
				"icon": "${PREFIX}/static/resources/lincs.gif",
